const unsigned char Rx = 96;
const unsigned char Ry = 64;
unsigned char LINEBUFFER[Rx];

void DrawPet()
{
    for (unsigned char frame = 0; frame < 8; frame++)
    {
        DrawScreen(frame, PET_SPRITE_XY, PET_SPRITE_ARR, 8);
        delay(PET_FRAME_DELAY);
    }
}

int death_delay = PET_DEATH_DELAY;

void DrawPetDeath()
{
    if (death_delay == 0)
    {
        for (unsigned char frame = 0; frame < 8; frame++)
        {
            DrawScreen(frame, DEATH_SPRITE_XY, DEATH_SPRITE_ARR, 8);
            delay(PET_FRAME_DELAY);
        }
        death_delay = PET_DEATH_DELAY;
    }
    else
    {
        death_delay--;
    }
}

void DrawScreen(unsigned char frame, const unsigned char *SPRITE_XY, const unsigned char *const *SPRITE_ARR, int numFrames)
{
    unsigned char SPRITE_FRAME;
    SPRITE_FRAME = frame % numFrames;
    unsigned char SPRITE_X, SPRITE_Y;
    SPRITE_X = pgm_read_byte_near(SPRITE_XY + (SPRITE_FRAME)*2);
    SPRITE_Y = pgm_read_byte_near(SPRITE_XY + (SPRITE_FRAME)*2 + 1);
    display.setX(0, Rx - 1);
    display.setY(0, Ry - 1);
    display.startData();
    unsigned char LINE_N, var;
    for (LINE_N = 0; LINE_N < Ry; LINE_N++)
    {
        for (var = 0; var < Rx; var++)
            LINEBUFFER[var] = BACKGROUND;
        DrawSprite((PGM_P)pgm_read_word(&(SPRITE_ARR[SPRITE_FRAME])), SPRITE_X, SPRITE_Y, Rx / 2 - SPRITE_X / 2, Ry / 2 - SPRITE_Y / 2, LINE_N);
        display.writeBuffer(LINEBUFFER, Rx);
    }
    display.endTransfer();
}
void DrawSprite(long SpritePointer, unsigned char SpriteSize_X, unsigned char SpriteSize_Y, unsigned char x, unsigned char y, unsigned char line_n)
{
    if (line_n < y || line_n >= y + SpriteSize_Y)
    {
        return;
    }
    unsigned char pos, pixel;
    for (pos = x; pos < x + SpriteSize_X; pos++)
    {
        pixel = pgm_read_byte_near(SpritePointer + SpriteSize_X * (line_n - y) + pos - x);
        if (pixel != ALPHA)
            LINEBUFFER[pos] = pixel;
    }
}
