void DrawDialogue(int dialogue)
{
  const unsigned char *SPRITE_XY;
  const unsigned char *const *SPRITE_ARR;

  switch (dialogue)
  {
  case HUNGER_DIALOGUE:
  {
    SPRITE_XY = HUNGER_SPRITE_XY;
    SPRITE_ARR = HUNGER_SPRITE_ARR;
    break;
  }
  case HEALTH_DIALOGUE:
  {
    SPRITE_XY = HEALTH_SPRITE_XY;
    SPRITE_ARR = HEALTH_SPRITE_ARR;
    break;
  }
  case HAPPINESS_DIALOGUE:
  {
    SPRITE_XY = HAPPINESS_SPRITE_XY;
    SPRITE_ARR = HAPPINESS_SPRITE_ARR;
    break;
  }
  }

  for (unsigned char frame = 0; frame < 4; frame++)
  {
    DrawScreen(frame, SPRITE_XY, SPRITE_ARR, 4);
    delay(DIALOGUE_FRAME_DELAY);
  }
  delay(DIALOGUE_DELAY);
}

void DisplayDialogue(int *currentHunger, int *currentHealth, int *currentHappiness)
{
  dialogueCycles--;

  if (dialogueCycles == 0)
  {
    if (*currentHunger < DIALOGUE_THRESHOLD)
    {
      DrawPet();
      display.clearScreen();
      DrawDialogue(HUNGER_DIALOGUE);
      display.clearScreen();
      DrawPet();
    }

    if (*currentHealth < DIALOGUE_THRESHOLD)
    {
      DrawPet();
      display.clearScreen();
      DrawDialogue(HEALTH_DIALOGUE);
      display.clearScreen();
      DrawPet();
    }
    if (*currentHappiness < DIALOGUE_THRESHOLD)
    {
      DrawPet();
      display.clearScreen();
      DrawDialogue(HAPPINESS_DIALOGUE);
      display.clearScreen();
      DrawPet();
    }
    dialogueCycles = NUM_DIALOGUE_CYCLES;
  }
}
